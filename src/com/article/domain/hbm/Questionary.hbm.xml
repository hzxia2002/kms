<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.article.domain">
	<class
		name="Questionary"
		table="questionary"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">questionary</meta>
        <meta attribute="tableComment">调查表</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system"></meta>

		<id
			name="id"
			type="long"
			column="ID"
		>
			<generator class="com.comet.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="title"
			column="TITLE"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">TITLE</meta>
            <meta attribute="ColumnComment">问卷名称</meta>
        </property>

		<property
			name="sponsor"
			column="SPONSOR"
			type="string"
			not-null="false"
			length="50"
        >
            <meta attribute="ColumnName">SPONSOR</meta>
            <meta attribute="ColumnComment">发起人</meta>
        </property>

		<property
			name="type"
			column="TYPE"
			type="java.lang.Long"
			not-null="false"
			length="10"
        >
            <meta attribute="ColumnName">TYPE</meta>
            <meta attribute="ColumnComment">类型：0问卷,1试卷</meta>
        </property>

		<property
			name="remark"
			column="REMARK"
			type="string"
			not-null="false"
			length="400"
        >
            <meta attribute="ColumnName">REMARK</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        >
            <meta attribute="ColumnName">CREATE_TIME</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">CREATE_USER</meta>
            <meta attribute="ColumnComment">创建人(记录帐号）</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        >
            <meta attribute="ColumnName">UPDATE_TIME</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">UPDATE_USER</meta>
            <meta attribute="ColumnComment">更新人(记录帐号）</meta>
        </property>

	
<!--

		<set name="userQuestionaries" inverse="true">
			<key column="QUESTIONARY_ID"/>
			<one-to-many class="UserQuestionary"/>
		</set>

-->
<!--

		<set name="charpters" inverse="true">
			<key column="QUESTIONARY_ID"/>
			<one-to-many class="Charpter"/>
		</set>

-->


	</class>	
    <query name="questionary_all_data_query">
        <![CDATA[FROM Questionary]]>
    </query>
</hibernate-mapping>