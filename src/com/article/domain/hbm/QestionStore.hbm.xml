<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.article.domain">
	<class
		name="QestionStore"
		table="qestion_store"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">question_store</meta>
        <meta attribute="tableComment">题库</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system"></meta>

		<id
			name="id"
			type="long"
			column="ID"
		>
			<generator class="com.comet.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="name"
			column="NAME"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">NAME</meta>
            <meta attribute="ColumnComment">题库名称</meta>
        </property>

		<property
			name="remark"
			column="REMARK"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">REMARK</meta>
            <meta attribute="ColumnComment">备注</meta>
        </property>

	
<!--

		<set name="questions" inverse="true">
			<key column="STORE_ID"/>
			<one-to-many class="Question"/>
		</set>

-->


	</class>	
    <query name="qestionstore_all_data_query">
        <![CDATA[FROM QestionStore]]>
    </query>
</hibernate-mapping>