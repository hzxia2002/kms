<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.article.domain">
	<class
		name="CharpterQuestion"
		table="charpter_question"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">charpter_question</meta>
        <meta attribute="tableComment">试卷问题</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system"></meta>

		<id
			name="id"
			type="long"
			column="ID"
		>
			<generator class="com.comet.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<many-to-one
			name="question"
			column="QUESTION_ID"
			class="Question"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="charpter"
			column="CHARPTER_ID"
			class="Charpter"
			not-null="false"
		>
		</many-to-one>


	</class>	
    <query name="charpterquestion_all_data_query">
        <![CDATA[FROM CharpterQuestion]]>
    </query>
</hibernate-mapping>