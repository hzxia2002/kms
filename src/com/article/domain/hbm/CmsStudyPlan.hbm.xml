<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.article.domain">
	<class
		name="CmsStudyPlan"
		table="cms_study_plan"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system"></meta>

		<id
			name="id"
			type="long"
			column="ID"
		>
			<generator class="com.comet.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="lastStudyTime"
			column="LAST_STUDY_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        />

		<property
			name="lastStudyDuration"
			column="LAST_STUDY_DURATION"
			type="long"
			not-null="false"
			length="19"
        />

		<property
			name="studyDuration"
			column="STUDY_DURATION"
			type="long"
			not-null="false"
			length="19"
        />

		<property
			name="studyTimes"
			column="STUDY_TIMES"
			type="long"
			not-null="false"
			length="19"
        />

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        />

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        />

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="19"
        />

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        />

		<many-to-one
			name="user"
			column="USER_ID"
			class="SysUser"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="course"
			column="COURSE_ID"
			class="CmsStudyCourse"
			not-null="false"
		>
		</many-to-one>


	</class>	
    <query name="cmsstudyplan_all_data_query">
        <![CDATA[FROM CmsStudyPlan]]>
    </query>
</hibernate-mapping>