<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.article.domain">
	<class
		name="Charpter"
		table="charpter"
	>
		
		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">charpter</meta>
        <meta attribute="tableComment">题目分类</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system"></meta>

		<id
			name="id"
			type="long"
			column="ID"
		>
			<generator class="com.comet.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="indexNo"
			column="INDEX_NO"
			type="long"
			not-null="false"
			length="19"
        >
            <meta attribute="ColumnName">INDEX_NO</meta>
            <meta attribute="ColumnComment">排序号</meta>
        </property>

		<property
			name="description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
			length="200"
        >
            <meta attribute="ColumnName">DESCRIPTION</meta>
            <meta attribute="ColumnComment">描述</meta>
        </property>

		<many-to-one
			name="questionary"
			column="QUESTIONARY_ID"
			class="Questionary"
			not-null="false"
		>
		</many-to-one>
	
<!--

		<set name="charpterQuestions" inverse="true">
			<key column="CHARPTER_ID"/>
			<one-to-many class="CharpterQuestion"/>
		</set>

-->


	</class>	
    <query name="charpter_all_data_query">
        <![CDATA[FROM Charpter]]>
    </query>
</hibernate-mapping>